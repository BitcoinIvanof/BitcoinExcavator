<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>

<GridPane fx:id="setupPage" fx:controller="com.bitcoin.controller.OptionsController"
          alignment="CENTER" prefHeight="552.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8"
          xmlns:fx="http://javafx.com/fxml/1">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="159.0" minWidth="10.0" prefWidth="159.0"/>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="95.0" minWidth="10.0" prefWidth="41.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <Label alignment="CENTER" prefHeight="25.0" prefWidth="252.0" text="%setupTitle" GridPane.columnSpan="2">
        <font>
            <Font size="16.0"/>
        </font>
    </Label>
    <Label text="%WorkLifetime" GridPane.rowIndex="1"/>
    <TextField fx:id="workLifetimeField" onKeyTyped="#keyTypedInWorkLifetimeField" GridPane.columnIndex="1"
               GridPane.rowIndex="1"/>
    <Label text="%DebuggingMode" GridPane.rowIndex="2"/>
    <CheckBox fx:id="debuggingModeTick" onAction="#actionInDebuggingModeTick" GridPane.columnIndex="1"
              GridPane.rowIndex="2"/>
    <Label text="%DebuggingTimer" GridPane.rowIndex="3"/>
    <CheckBox fx:id="debuggingTimerTick" onAction="#actionInDebuggingTimerTick" GridPane.columnIndex="1"
              GridPane.rowIndex="3"/>
    <Label text="%EnabledDevices" GridPane.rowIndex="4"/>
    <TextField fx:id="enabledDevicesField" onKeyTyped="#keyTypedInEnabledDevicesField" GridPane.columnIndex="1"
               GridPane.rowIndex="4"/>
    <Label text="%GpuTargetFps" GridPane.rowIndex="5"/>
    <TextField fx:id="gpuTargetFpsField" onKeyTyped="#keyTypedInGpuTargetFpsField" GridPane.columnIndex="1"
               GridPane.rowIndex="5"/>
    <Label text="%GpuTargetFpsBase" GridPane.rowIndex="6"/>
    <TextField fx:id="gpuTargetFpsBaseField" onKeyTyped="#keyTypedInGpuTargetFpsBaseField"
               GridPane.columnIndex="1" GridPane.rowIndex="6"/>
    <Label text="%GpuForceWorkSize" GridPane.rowIndex="7"/>
    <TextField fx:id="gpuForceWorkSizeField" onKeyTyped="#keyTypedInGpuForceWorkSizeField"
               GridPane.columnIndex="1" GridPane.rowIndex="7"/>
    <Label text="%GpuVectors" GridPane.rowIndex="8"/>
    <TextField fx:id="gpuVectorsField" onKeyTyped="#keyTypedInGpuVectorsField" GridPane.columnIndex="1"
               GridPane.rowIndex="8"/>
    <Label text="%GpuNoArray" GridPane.rowIndex="9"/>
    <CheckBox fx:id="gpuNoArrayTick" onAction="#actionInGpuNoArrayTick" GridPane.columnIndex="1"
              GridPane.rowIndex="9"/>
    <Label text="%GpuDebugSource" GridPane.rowIndex="10"/>
    <CheckBox fx:id="gpuDebugSourceTick" onAction="#actionInGpuDebugSourceTick" GridPane.columnIndex="1"
              GridPane.rowIndex="10"/>
</GridPane>